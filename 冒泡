#include <stdio.h>
#include <ctype.h>
#include <math.h>
//冒泡排序
int main()
{
	int a[10]={1,5,8,9,2,3,0,4,6,7};
	for (int i=0;i<10;i++){
		//TODO
		for (int j =0;j<10-1-i;j++){
			if (a[j]<a[j+1]){
				int temp=0;
				temp=a[j+1];
				a[j+1]=a[j];
				a[j]=temp;
			}
		}
	}
	for (int i=9;i>=0;i--){
		printf("%d\n",a[i]);	
		//TODO
	}
    return 0;
}	
